/**app.wxss**/
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.canvas {
  position: absolute;
  top: -1000px;
  left: -1000px;                  
}   
image {
  vertical-align:top;
}
view,image,text,form,button {
  margin: 0;
  padding: 0;
}
.btn-reset{
  padding: 0;
  margin: 0;
  position: static; 
  border: none;
  color: #fff;
  text-align: center;
}
.btn-reset::after{
  border: none;
  text-shadow: none;
}
.clearfix{
  content: '';
  clear: both;
  display: block;
}
.your-decsion {
  width: 555rpx;
  height: auto;
  font-size: 0;
  margin: 45rpx auto;
}
.click-form {
  width: 250rpx;
  height: 70rpx;
  display: inline-block;
}
.left-m {
  margin-right: 50rpx;
}
.btn-face {
  width: 250rpx;
  height: 70rpx;
  font-size: 30rpx;
  line-height: 70rpx;
  text-align: center;
  border-radius: 0;
  position: relative;
  border: none;
  display: block;
  margin: 0 auto;
  color: #fff;
  background-color: transparent;
  background-image: url('data:image/png;base64,');
  background-size: 100% 100%;
}
.btn-left {
  width: 250rpx;
  height: 70rpx;
  font-size: 30rpx;
  line-height: 70rpx;
  border-radius: 0;
  position: relative;
  border: none;
  display: block;
  margin: 0 auto;
  color: #000;
  background-color: transparent;
  background-image: url('data:image/png;base64,');
  background-size: 100% 100%;
}
.keep-img {
  width: 40vw;
  height: 40vw;
  background: #fafafa;
  margin: -25% auto;  
  position: relative; 
  border-radius: 10rpx;
  box-shadow: 0 3rpx 30rpx #888888;
}
.gain-img {
  width: 40vw;
  height: 40vw;
  background: #fff;
}
.select {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  bottom: -20rpx;
  right: -20rpx;
}
.keep-text {
  margin-top: 30%;
  text-align: center;
}
.keep-text {
  margin-top: 30%;
  text-align: center;
}
.keep-text text {
  color: #000;
  font-size: 20rpx;
} 
.koopoo {
  width: 100%;
  margin: 0 auto 30rpx;
  text-align: center;
}
.logo {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  display: block;
  margin: 0 auto;
}
.logo-text {
  font-size: 30rpx;
  color: #999;
  text-align: center;
}