/**index.wxss**/
.container {
  width: 100%;
  height: auto;
  display: block;
  background-image: url('data:image/jpeg;base64,');
  background-repeat: repeat;
  background-position: 50% 50%;
  position: relative;
}
.move {
  height: 100%;
  overflow: hidden;
}
.show-time {
  width: 100vw;
  height: 100vh;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1000;
}
.can {
  display: none;
}
.box {
  width: 100%;
  height: 100vh;
  font-size: 0;
}
.index-bear {
  width: 100%;
  height: 50%;
  display: block; 
}
.add-face {
  width: 40%;
  height: 450rpx;
  display: inline-block;
  margin: -160rpx 0 0 6.5%;
  border-radius: 12rpx;
  float: left;
  background-image: url('data:image/jpeg;base64,');
  background-size: 100% 100%;
  text-align: center;
  box-shadow: 0 5rpx 25rpx #888888;
}
.add-huge {
  width: 40%;
  height: 450rpx;
  display: inline-block;
  margin: -160rpx 0 0 6.5%;
  background-size: 100% 100%;
  border-radius: 12rpx;
  float: left;
  background-image: url('data:image/jpeg;base64,');
  background-size: 100% 100%;
  text-align: center;
  box-shadow: 0 5rpx 25rpx #888888;
  position: relative;
}
.index-img {
  width: 200rpx;
  height: 200rpx;
  background: #ccc;
  border-radius: 50%;
  display: block;
  margin: 48rpx auto;
}
.huge-img {
  width: 54rpx;
  height: 78rpx;
  position: absolute;
  top: 180rpx;
  right: 50rpx;
}
.btn-face {
  color: #fff;
  /* background: #000; */
}
.click-text {
  width: 100%;
  height: 40rpx;
  font-size: 30rpx;
  line-height: 40rpx;
  color: #969899;
  margin-top: 20rpx;
  text-align: center;
  display: block;
}
.free-loop {
  width: 100%;
  margin-top: 370rpx;
}
.loop-title {
  width: 80%;
  margin: 0 auto; 
  text-align: center;
}
.loop-text {
  width: 80%;
  height: 90rpx;
  margin: 0 auto;
  font-size: 30rpx;
  color: #999;
  line-height: 90rpx;
  background-image: url('data:image/png;base64,');
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
.bot-title {
  color: #ccc;
  font-size: 22rpx;
  text-align: center;
  margin: 10rpx 0 30rpx 0;
}
.free-box {
  width: 90vw;
  height: auto;
  margin: 30rpx auto 60rpx;
  font-size: 0;
  padding: 0;
  display: block;
}
.free-detail {
  width: 42vw;
  height: 380rpx;
  display: inline-block; 
  border-radius: 10rpx; 
  overflow: hidden;
  margin: 0 0 4% 2vw; 
  box-shadow: 0 5rpx 25rpx #888888;
  vertical-align: top;
}
.img-detail {
  width: 42vw;
  height: 34vw;
}
.goods-detail {
  width: 100%;
  height: calc(400rpx - 40vw);
  padding: 10rpx 0 0 20rpx;
  position: relative;
}
.gloabl-goods {
  font-size: 24rpx;
  height: 60rpx; 
  line-height: 30rpx;  
  color:#000;
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;  
  margin-right: 70rpx;
}
.goods-size {
  font-size: 20rpx;
  color: #ccc;
}
.free-img {
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  top: 11%;
  right: 30rpx;
}
.price {
  font-size: 18rpx;
  color: red;
}
.money {
  font-size: 26rpx;
  color: red;
  font-weight: bold;
  margin-right: 10rpx;
}
.prices {
  font-size: 18rpx;
  color: #333;
  text-decoration: line-through;
}
.user-promot {
  width: 100vw;
  height: 100vh;
  background: rgba(0,0,0,0.3); 
  position: absolute;
  top: 0;
  left: 0;
  z-index: 100;
}
.hidden {
  display: none;
}
.direction {
  width: 100%;
  height: 40vh;
  margin-top: 40vh;
  position: relative;
}
.promot-infor {
  width: 70%;
  text-align: center;
  color: #fff;
  font: normal normal bolder 45rpx sans-serif;
  display: block;
  margin: 0 auto;
}
.img-promot {
  width: 150rpx;
  height: 147rpx;
  position: absolute;
  top: -100rpx;
  right: 0;
  bottom: 0;
  left: 0;
  margin: auto;
  animation: move 0.8s linear infinite;
}
@-webkit-keyframes move{
  0% {
    top: 200rpx
  }
  50% {
    top: 0rpx;
  }
  100% {
    top: -200rpx;
  }
}

