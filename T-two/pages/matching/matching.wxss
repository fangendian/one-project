.shareThings {
  width: 100%;
  font-size: 18px;
  font-family: '微软雅黑';
  text-align: center;
  color: #000;
  margin: 30rpx auto;
}
.free-box {
  width: 90vw;
  height: auto; 
  margin: 0 auto;
  font-size: 0;
  padding: 0;
  display: block;
}
.free-detail {
  width: 42vw;
  height: 380rpx;
  display: inline-block; 
  border-radius: 10rpx; 
  overflow: hidden;
  margin: 0 0 4% 2vw; 
  box-shadow: 0 3rpx 25rpx #888888;
  vertical-align: top;
}
.img-detail {
  width: 42vw;
  height: 34vw;
}
.goods-detail {
  width: 90%;
  height: calc(400rpx - 40vw);
  background: #fff;
  padding: 10rpx 0 0 20rpx;  
  position: relative;
  overflow:hidden; 
}
.gloabl-goods {
  font-size: 24rpx;
  height: 60rpx; 
  line-height: 30rpx;  
  color:#000;
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;  
  margin-right: 70rpx;
  /* background: aqua;  */
}
.free-img {
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  top: 20%;
  right: 10rpx;
}
.price {
  font-size: 18rpx;
  color: red;
}
.money {
  font-size: 26rpx;
  color: red;
  font-weight: bold;
  margin-right: 10rpx;
}
.prices {
  font-size: 18rpx;
  color: #333;
  text-decoration: line-through;
}